<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddSevlet</servlet-name>
        <servlet-class>next.record.AddSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addRecords</servlet-name>
        <servlet-class>addRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getRecords</servlet-name>
        <servlet-class>getRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>edit_return</servlet-name>
        <servlet-class>edit_return</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateRecords</servlet-name>
        <servlet-class>updateRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteRecords</servlet-name>
        <servlet-class>deleteRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validateLogin</servlet-name>
        <servlet-class>Login.validateLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginValidate</servlet-name>
        <servlet-class>LoginValidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddSevlet</servlet-name>
        <url-pattern>/AddSevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addRecords</servlet-name>
        <url-pattern>/addRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getRecords</servlet-name>
        <url-pattern>/getRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>edit_return</servlet-name>
        <url-pattern>/edit_return</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateRecords</servlet-name>
        <url-pattern>/updateRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteRecords</servlet-name>
        <url-pattern>/deleteRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validateLogin</servlet-name>
        <url-pattern>/validateLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginValidate</servlet-name>
        <url-pattern>/LoginValidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
